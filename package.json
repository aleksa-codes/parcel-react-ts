{
  "name": "react-ts-parcel",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html -p 3000",
    "prebuild": "npx rimraf dist",
    "build": "yarn prebuild && parcel build index.html --public-url ./ --no-cache",
    "dev": "parcel index.html -p 3000 --open"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "parcel": "^2.8.2",
    "process": "^0.11.10"
  },
  "@parcel/transformer-js": {
    "inlineFS": false,
    "inlineEnvironment": false
  }
}
